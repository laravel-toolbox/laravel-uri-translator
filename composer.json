{
    "name": "tool-mountain/laravel-uri-translator",
    "description": "Translate a URI or individual slugs.",
    "keywords": [
        "php",
        "laravel",
        "locale",
        "language",
        "country",
        "localization",
        "url",
        "uri",
        "slugs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Vermeyen",
            "email": "ivan@codezero.be"
        },
        {
            "name": "Roj Vroemen",
            "email": "roj@vroe.men"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "ToolMountain\\UriTranslator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToolMountain\\UriTranslator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "ToolMountain\\UriTranslator\\UriTranslatorServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
